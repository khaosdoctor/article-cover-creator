{
  "deno.enable": true,
  "deno.unstable": false,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "prettier.enable": false,
  "eslint.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  }
}
